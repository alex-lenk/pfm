{
  "name": "gulp-template",
  "version": "1.0.0",
  "description": "",
  "main": "gulpfile.js",
  "scripts": {
    "start": "gulp start",
    "build": "gulp build",
    "lighthouse": "gulp build && gulp lighthouse",
    "test": "editorconfig-cli & stylelint ./src/styles/**/*.scss --syntax scss & eslint ./src/js",
    "stylelint-fix": "stylelint ./src/styles/**/*.scss --fix --syntax scss",
    "eslint-fix": "eslint ./src/js/** --fix"
  },
  "keywords": [],
  "author": "Denys Vykhrystiuk",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "latest",
    "@babel/preset-env": "latest",
    "@htmlacademy/editorconfig-cli": "latest",
    "babel-loader": "latest",
    "browser-sync": "latest",
    "chrome-launcher": "latest",
    "circular-dependency-plugin": "latest",
    "del": "latest",
    "duplicate-package-checker-webpack-plugin": "latest",
    "editorconfig-cli": "latest",
    "eslint": "latest",
    "eslint-config-htmlacademy": "latest",
    "eslint-config-standard": "latest",
    "eslint-plugin-import": "latest",
    "eslint-plugin-node": "latest",
    "eslint-plugin-promise": "latest",
    "eslint-plugin-standard": "latest",
    "gulp": "latest",
    "gulp-autoprefixer": "latest",
    "gulp-clean-css": "latest",
    "gulp-eslint": "latest",
    "gulp-html-bem-validator": "latest",
    "gulp-imagemin": "latest",
    "gulp-npm-dist": "latest",
    "gulp-npm-files": "latest",
    "gulp-plumber": "latest",
    "gulp-pug": "latest",
    "gulp-pug-linter": "latest",
    "gulp-rename": "latest",
    "gulp-sass": "latest",
    "gulp-shorthand": "latest",
    "gulp-sourcemaps": "latest",
    "gulp-stylelint": "latest",
    "gulp-svgstore": "latest",
    "gulp-w3c-html-validator": "latest",
    "husky": "latest",
    "lighthouse": "latest",
    "node-sass": "latest",
    "open": "latest",
    "stylelint": "latest",
    "stylelint-config-htmlacademy": "latest",
    "stylelint-config-standard": "latest",
    "stylelint-config-standard-scss": "latest",
    "stylelint-scss": "latest",
    "webpack-stream": "latest"
  },
  "dependencies": {
    "jquery": "latest",
    "normalize.css": "latest",
    "slick-carousel": "latest",
    "bootstrap": "^5.0.0-alpha3"
  },
  "editorconfig-cli": [
    "*.json",
    "*.js",
    "src/*.html",
    "src/js/**/*.js",
    "src/img/**/*.svg",
    "src/styles/**/*.{sass,scss}"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "npm run test && npm run build"
    }
  }
}
